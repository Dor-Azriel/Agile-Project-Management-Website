pipeline {
    agent { docker { image 'python:3.7' } }
    triggers {
      githubPush()
  }

    stages {
        stage('Build') {
            steps {
                sh 'python --version'
            }

        }
        stage('setup'){
          steps{
            sh 'python -m pip3 install -r requirements.txt --user'
            echo 'hello'
          }

        }

    }

}